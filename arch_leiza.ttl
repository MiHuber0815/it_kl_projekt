    skos:scopeNote "Geistliche haben sich häufig mit der Altertumskunde beschäftigt." ;
    skos:broader "berufsrolle" ;
    skos:inScheme arch: .
arch:geograf a skos:Concept ;
skos:prefLabel_de "Geograf*in" ;
    skos:altLabel_de "Erdkundler*in" ;
    dct:description_de "Wissenschaftler*in, die sich mit der Erforschung der Erdoberfläche beschäftigt." ;
    skos:definition_de "Person, die sich mit der Beschreibung und Analyse der räumlichen Strukturen und Prozesse auf der Erde beschäftigt." ;
    skos:broader "berufsrolle" ;
    skos:inScheme arch: .
